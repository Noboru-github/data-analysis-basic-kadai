        "import numpy as np\n",
        "\n",
        "A = np.array([[0, 1], [2, 3], [4, 5]])\n",
        "B = np.array([[0, 1, 2, 3], [4, 5, 6, 7]])\n",
        "\n",
        "np.dot(A, B)\n",
        "\n",
        "AB = np.dot(A, B)\n",
        "\n",
        "print(f\"AとBの行列の積は{AB}です。\")\n",
        "\n",
        "AB.max()"
      ]
